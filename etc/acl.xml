<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="Magenest_Messenger::chatbox" title="Facebook Messenger Chatbox" sortOrder="1000">
                        <resource id="Magenest_Messenger::dashboard_data" title="Dashboard"/>
                        <resource id="Magenest_Messenger::messengersupport" title="Messenger Support Live"/>
                        <resource id="Magenest_Messenger::stat" title="Manage Data User"/>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Magenest_Messenger::config_api" title="Facebook Messenger Support Configuration"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>